<Window x:Class="Runge_Kutta.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Runge_Kutta"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="131*"/>
            <ColumnDefinition Width="29*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="327*"/>
            <RowDefinition Height="107"/>
        </Grid.RowDefinitions>
        <Canvas x:Name="MainCanvas" Grid.Row="0" Margin="4px" />
        <TextBox x:Name="TBPoints" Grid.Column="1" IsReadOnly="True"/>
        <StackPanel Grid.Row="1" Grid.Column="1" Orientation="Vertical">
            <Button Content="Runge-Kutta" Click="RK_Click"/>
            <Button Content="Сложное уравнение" Click="Hard_Click"/>
        </StackPanel>
        <Frame x:Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
