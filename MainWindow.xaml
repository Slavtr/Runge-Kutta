<Window x:Class="Runge_Kutta.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Runge_Kutta"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="131*"/>
            <ColumnDefinition Width="29*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="19*"/>
            <RowDefinition Height="308*"/>
            <RowDefinition Height="107"/>
        </Grid.RowDefinitions>
        <Menu Height="17" VerticalAlignment="Center" Grid.Row="0" Grid.ColumnSpan="2">
            <MenuItem Header="Рунге-Кутта" Click="RK_Click"></MenuItem>
            <MenuItem Header="Сложное уравнение" Click="Hard_Click"></MenuItem>
            <MenuItem Header="Справка" Click="MenuItem_Click"></MenuItem>
        </Menu>
        <Canvas x:Name="MainCanvas" Margin="4,4,4,4" Grid.Row="1"/>
        <TextBox x:Name="TBPoints" Grid.Column="1" IsReadOnly="True" VerticalScrollBarVisibility="Auto" Grid.RowSpan="2" Grid.Row="1"/>
        <Frame x:Name="MainFrame" Grid.Row="2" NavigationUIVisibility="Hidden" Margin="0,3,0,0"/>
    </Grid>
</Window>
